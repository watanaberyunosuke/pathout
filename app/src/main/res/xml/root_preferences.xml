<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory app:title="Navigation">
        <ListPreference
            app:entries="@array/navigation_method_entries"
            app:entryValues="@array/navigation_method_entries"
            app:key="nav_method_key"
            app:title="Navigation Method"
            app:useSimpleSummaryProvider="true" />

    </PreferenceCategory>

    <PreferenceCategory app:title="Customization">
        <ListPreference
            app:entries="@array/map_styles"
            app:entryValues="@array/map_styles"
            app:key="main_map_style_key"
            app:title="Main Map Style"
            app:useSimpleSummaryProvider="true" />


    </PreferenceCategory>

</PreferenceScreen>
